#include <stdio.h>

int main() {
    char ch;

    printf("Enter a character: ");
    scanf(" %c", &ch);

    
    if ((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z')) {
        
        
        if (ch >= 'A' && ch <= 'Z') {
            ch = ch + 32;
        }

        
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
            printf("It is a Vowel.\n");
        } else {
            printf("It is a Consonant.\n");
        }
    } else {
        printf("Invalid input! Please enter an alphabet.\n");
    }

    return 0;
}

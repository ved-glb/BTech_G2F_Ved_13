#include <stdio.h>

int main() {
    int age;

    printf("Enter age: ");
    scanf("%d", &age);

    if (age < 0) {
        printf("Invalid age entered.");
    }
    else if (age <= 12) {
        printf("Category: Child");
    }
    else if (age <= 18) {
        printf("Category: Teen");
    }
    else if (age <= 60) {
        printf("Category: Adult");
    }
    else {
        printf("Category: Senior Citizen");
    }

    return 0;
}
